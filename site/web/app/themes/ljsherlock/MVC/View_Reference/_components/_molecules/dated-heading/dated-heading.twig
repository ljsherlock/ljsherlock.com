{% embed 'component.twig' with { el: 'div', bem : [{block: 'dated-heading'}] } %}
   {% block component %}

      {% include 'time.twig' with { bem: [
         { block:'dated-heading', element: 'date' }
      ], attrs: [{ datetime: '01-02-1990' }] } %}

      {% include 'typography.twig' with {
         el|default('h1'),
         bem: [ { block:'dated-heading', element: 'heading' } ] } %}

   {% endblock %}
{% endembed %}
