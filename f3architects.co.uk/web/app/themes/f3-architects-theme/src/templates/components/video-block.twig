{# base/components/video/video.twig #}

{#
   Video

   @bem {Array} ()
    {
       block {String}
       element {String}
       modifier {Array}
    }
    @attrs (Array)
    @sources (array)
    {
      src
    }
#}

{# Parameters #}
{% set videoSources = sources ?? null %}
{% set videoImg = img %}
{% set videoAttrs = attrs ?? null %}
{% set videoBem = bem ?? null %}

{# Variables #}
{% set videoBemDefault = [{ block: 'video' , modifier: ['block'] }] %}
{% set videoBem = (videoBem) ? videoBem|merge( videoBemDefault ) : videoBemDefault %}

{# Output #}
{% if videoSources %}

   {% import 'macros.twig' as mcro %}

   {% set videoBlockInnerHTML %}

      {% include 'components/video.twig' with { sources: videoSources, img: videoImg, attrs: {}, bem: [] } %}

      <button class="video__playpause " id="playpause">
          <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 500 500" style="width:150px; height:150px;"><image data-name="Vector Smart Object" x="7" y="7" width="486" height="486" xlink:href="data:img/png;base64,"/></svg>
      </button>

   {% endset %}

   {{ mcro.createHTMLTag( 'div', videoAttrs, videoBem, videoBlockInnerHTML ) }}

 {% endif %}
